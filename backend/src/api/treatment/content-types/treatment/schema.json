{
  "kind": "collectionType",
  "collectionName": "treatments",
  "info": {
    "singularName": "treatment",
    "pluralName": "treatments",
    "displayName": "~ Treatments",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "regex": "^/[a-zA-Z0-9]+(-[a-zA-Z0-9]+)*$",
      "default": "/",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "themeColor": {
      "type": "enumeration",
      "enum": [
        "midnight-950",
        "blue-400",
        "green-400",
        "lime-400",
        "violet-400"
      ],
      "default": "midnight-950",
      "required": true
    },
    "excerpt": {
      "type": "text"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "blocks": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "__comment": "Blocks is dynamic populated by ContentModule",
      "components": [
        "blocks.archive-posts",
        "blocks.archive-treatments",
        "blocks.benefits",
        "blocks.brain-image-map",
        "blocks.depositions",
        "blocks.faq",
        "blocks.form-contact",
        "blocks.form-partnership",
        "blocks.headline-contact",
        "blocks.headline-posts",
        "blocks.image-slider",
        "blocks.main-hero",
        "blocks.medium-hero",
        "blocks.neuro-types",
        "blocks.rich-text",
        "blocks.short-hero",
        "blocks.team",
        "blocks.treatment-guide",
        "blocks.whatsapp-floating-cta",
        "wrappers.close",
        "wrappers.background",
        "wrappers.join"
      ]
    },
    "pageSeo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "component": "shared.seo"
    },
    "publishedDateTime": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "treatments"
    }
  }
}
