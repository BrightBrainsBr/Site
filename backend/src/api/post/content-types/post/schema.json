{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "~ Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "regex": "^/[a-zA-Z0-9]+(-[a-zA-Z0-9]+)*$",
      "default": "/",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredImage": {
      "type": "component",
      "repeatable": false,
      "component": "common.responsive-image"
    },
    "excerpt": {
      "type": "text",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "posts"
    },
    "treatments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::treatment.treatment",
      "inversedBy": "posts"
    },
    "blocks": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "blocks-posts.button",
        "blocks-posts.image",
        "blocks-posts.text",
        "blocks-posts.video"
      ]
    },
    "pageSeo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "component": "shared.seo"
    },
    "publishedDateTime": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    }
  }
}
